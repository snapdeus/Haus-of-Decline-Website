                    <div id="comicCarousel" data-interval="false" class="carousel slide lazy-load" data-ride="carousel">
                        <div class="carousel-inner">
                            <a href="/comics/gay/1">
                                <% gayComics.forEach((img, i)=> { %>
                                    <div class="carousel-item <%= i === 0 ? 'active' : ''%>">
                                        <% if (i===0){ %>
                                            <img src="/uploads/GayComics/<%= img.filename%>"
                                                class="d-block img-thumbnail specialBorder"
                                                alt="<%= img.description %>">
                                            <% } else { %>
                                                <img data-src="/uploads/GayComics/<%= img.filename%>"
                                                    class="d-block img-thumbnail specialBorder"
                                                    alt="<%= img.description %>">
                                                <% } %>
                                    </div>
                                    <% }) %>
                            </a>
                        </div>
                        <% if(gayComics.length> 1) {%>
                            <a id="clickIcon" class="carousel-control-prev" href="#comicCarousel" role="button"
                                data-slide="prev">
                                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                <span class="sr-only">Previous</span>
                            </a>
                            <a id="clickIcon" class="carousel-control-next" href="#comicCarousel" role="button"
                                data-slide="next">
                                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                <span class="sr-only">Next</span>
                            </a>
                            <% } %>
                    </div>
                </div>
            </div>
